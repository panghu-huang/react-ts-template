{
  "compilerOptions": {
    "checkJs": false,
    "allowJs": false,
    "baseUrl": ".",
    "rootDir": ".",
    "noImplicitAny": true,
    "noImplicitThis": true,
    "jsx": "react",
    "moduleResolution": "Node",
    "module": "CommonJS",
    "target": "ES5",
    "esModuleInterop": true
  },
  "exclude": [
    "config",
    "public",
    "dist"
  ]
}